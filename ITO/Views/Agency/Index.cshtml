@model IEnumerable<ITO.Models.Agency>
@{
    ViewBag.Title = "Все учреждения";
}
<a asp-action="Create">Добавить учреждение</a>
<table class="table">

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Id">Подробнее</a> |                
                <a asp-action="Delete"  asp-route-id="@item.Id">Удалить</a>
            </td>
        </tr>
     }

</table>